    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>e2126c47-1d73-4388-8ccb-f212ed75a807</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
